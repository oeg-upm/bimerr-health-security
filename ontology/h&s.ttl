@prefix : <http://www.semanticweb.org/serge30/hs#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/serge30/hs> .

<http://www.semanticweb.org/serge30/hs> rdf:type owl:Ontology ;
                                         rdfs:comment "This ontology model the H&S domain"@en .

#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/serge30/hs#causedBy
:causedBy rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/serge30/hs#generates
:generates rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/serge30/hs#hasDuration
:hasDuration rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/serge30/hs#hasLocation
:hasLocation rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/serge30/hs#involves
:involves rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/serge30/hs#message
:message rdf:type owl:DatatypeProperty .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/serge30/hs#Alert
:Alert rdf:type owl:Class ;
       rdfs:subClassOf [ rdf:type owl:Restriction ;
                         owl:onProperty :message ;
                         owl:someValuesFrom xsd:string
                       ] ;
       rdfs:label "Alert"@en .


###  http://www.semanticweb.org/serge30/hs#HazardType
:HazardType rdf:type owl:Class ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty :causedBy ;
                              owl:someValuesFrom :TriggerEvent
                            ] ;
            rdfs:label "Hazard Type"@en .


###  http://www.semanticweb.org/serge30/hs#InternalLocation
:InternalLocation rdf:type owl:Class ;
                  rdfs:label "Internal Location"@en .


###  http://www.semanticweb.org/serge30/hs#TriggerEvent
:TriggerEvent rdf:type owl:Class ;
              rdfs:subClassOf [ rdf:type owl:Restriction ;
                                owl:onProperty :generates ;
                                owl:someValuesFrom :Alert
                              ] ;
              rdfs:label "Trigger Event"@en .


###  http://www.semanticweb.org/serge30/hs#WorkProcess
:WorkProcess rdf:type owl:Class ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty :hasDuration ;
                               owl:someValuesFrom <http://www.w3.org/2006/time#TemporalEntity>
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :hasLocation ;
                               owl:someValuesFrom :InternalLocation
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :involves ;
                               owl:someValuesFrom :TriggerEvent
                             ] ;
             rdfs:label "Work Process"@en .


###  http://www.w3.org/2006/time#TemporalEntity
<http://www.w3.org/2006/time#TemporalEntity> rdf:type owl:Class ;
                                             rdfs:label "Temporal Entity"@en .


###  http://www.w3.org/2006/time#TimeInterval
<http://www.w3.org/2006/time#TimeInterval> rdf:type owl:Class ;
                                           rdfs:subClassOf <http://www.w3.org/2006/time#TemporalEntity> ;
                                           rdfs:label "Time Interval"@en .


